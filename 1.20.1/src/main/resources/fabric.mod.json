{
  "schemaVersion": 1,
  "id": "cool-resource-finder",
  "version": "${version}",
  "name": "Cool Resource Finder",
  "description": "Special compass that allows to locate resources.",
  "authors": [
    "https://github.com/hellozyemlya"
  ],
  "contact": {
    "homepage": "https://github.com/hellozyemlya/cool-resource-finder",
    "sources": "https://github.com/hellozyemlya/cool-resource-finder"
  },
  "license": "CC0-1.0",
  "icon": "assets/cool-resource-finder/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "value": "hellozyemlya.resourcefinder.ResourceFinder",
        "adapter": "kotlin"
      }
    ],
    "client": [
      {
        "value": "hellozyemlya.resourcefinder.ResourceFinderClient",
        "adapter": "kotlin"
      }
    ],
    "fabric-datagen": [
      {
        "value": "hellozyemlya.resourcefinder.ResourceFinderDataGenerator",
        "adapter": "kotlin"
      }
    ]
  },
  "mixins": [
    "cool-resource-finder.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.21",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "fabric-api": "*",
    "fabric-language-kotlin": ">=1.9.0"
  },
  "suggests": {
    "another-mod": "*"
  }
}